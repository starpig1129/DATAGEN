{
  "name": "multi-agent-analysis-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "Vue 3多代理數據分析系統前端",
  "author": "Multi-Agent Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "echo 'Skipping husky install in subdirectory'"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "@element-plus/icons-vue": "^2.1.0",
    "@types/d3": "^7.4.0",
    "@types/dompurify": "^3.0.0",
    "@types/lodash-es": "^4.17.0",
    "@types/plotly.js": "^2.12.0",
    "@vue/apollo-composable": "^4.0.0",
    "d3": "^7.8.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.0",
    "dompurify": "^3.0.0",
    "element-plus": "^2.4.0",
    "file-saver": "^2.0.5",
    "graphql": "^16.8.0",
    "graphql-ws": "^5.14.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lodash-es": "^4.17.0",
    "marked": "^9.1.0",
    "pinia": "^2.1.0",
    "plotly.js": "^2.27.0",
    "quill": "^2.0.3",
    "vue": "^3.4.0",
    "vue-i18n": "^9.14.4",
    "vue-plotly": "^1.1.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^3.1.4",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.0",
    "husky": "^8.0.0",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.0",
    "unplugin-auto-import": "^0.17.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "*.{vue,js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
